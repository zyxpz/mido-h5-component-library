{
  "name": "mido-h5-component-library",
  "version": "1.0.0",
  "description": "h5组件库和一些公用方法",
  "main": "index.js",
  "entry": {
    "main": "./src/pages/main.js"
  },
  "scripts": {
    "dev": "npm run user-setting && npm run all",
    "user-setting": "node ./config/user.setting.config.js",
    "sever": "node ./config/webpack.dev.config.js",
    "lint": "esw ./config/*.config.* ./src/pages/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --fix --watch",
    "lint:style": "stylelint --fix --syntax less src",
    "lint-staged": "lint-staged",
    "all": "npm-run-all --parallel --race --continue-on-error lint:watch sever"
  },
  "author": "Midnight Blues",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.11.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-markdown": "^1.0.0-rc.0",
    "eslint-watch": "^4.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^7.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "my-ip": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-smart-import": "^0.7.6",
    "precss": "^4.0.0",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "lint-staged": {
    "{src,config}/**/*.{js,jsx,vue}": [
      "esw --fix",
      "git add"
    ],
    "*.md": [
      "esw --ext '.md' --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix --syntax scss",
      "git add"
    ]
  },
  "files": [
    "bin",
    "lib",
    "package.json",
    "README.md"
  ],
  "engines": {
    "npm": ">=3.10.2",
    "node": ">=8.0.0"
  },
  "ci": {
    "versions": [
      "4",
      "6"
    ]
  }
}
